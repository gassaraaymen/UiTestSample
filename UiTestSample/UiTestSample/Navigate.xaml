<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UiTestSample.Navigate">
  <ContentView.Content>
      <StackLayout>
            <Button Text="Navigate To Elements" Clicked="Navigate_Element_Command" AutomationId="ButtonElement" ></Button>
            <Button Text="Navigate To cars Gallery" Clicked="Navigate_Command" AutomationId="ButtonGallery"></Button>
            <Button Text="Navigate To Radio Button" Clicked="Navigate_Radio_Command" AutomationId="ButtonRadio"></Button>
        </StackLayout>
  </ContentView.Content>
</ContentView>